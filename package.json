{
  "name": "mommy-ai",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "apps/*"
  ],
  "scripts": {
    "dev:api": "npm run dev --workspace=@mommy-ai/api",
    "dev:mobile": "npm run start --workspace=@mommy-ai/mobile",
    "build:api": "npm run build --workspace=@mommy-ai/api",
    "start:api": "npm run start --workspace=@mommy-ai/api",
    "install:all": "npm install",
    "clean": "rimraf node_modules apps/*/node_modules"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "rimraf": "^5.0.5"
  }
}